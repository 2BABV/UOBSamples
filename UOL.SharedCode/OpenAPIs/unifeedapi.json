      "EtimClassModel": {
        "type": "object",
        "properties": {
          "selected": {
            "type": "boolean",
            "description": "Selected"
          },
          "code": {
            "maxLength": 8,
            "minLength": 8,
            "type": "string",
            "description": "ETIM Product Class Code e.g. \"EC000001\"",
            "nullable": true
          },
          "description": {
            "maxLength": 80,
            "minLength": 0,
            "type": "string",
            "description": "ETIM Product Class Description e.g. \"Busbar terminal\"",
            "nullable": true
